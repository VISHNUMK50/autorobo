<node args="Rev184_position_controller Rev185_position_controller Rev186_position_controller Rev187_position_controller Rev197_position_controller joint_state_controller " name="controller_spawner" ns="autorobo" output="screen" pkg="controller_manager" respawn="false" type="spawner"/>
